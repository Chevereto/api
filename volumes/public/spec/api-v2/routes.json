{
    "name": "api-v2",
    "spec": "\/spec\/api-v2\/routes.json",
    "routes": {
        "api-v2:\/api\/2\/image\/base64\/": {
            "name": "api-v2:\/api\/2\/image\/base64\/",
            "spec": "\/spec\/api-v2\/api\/2\/image\/base64\/route.json",
            "path": "\/api\/2\/image\/base64\/",
            "regex": "#\/api\/2\/image\/base64\/#",
            "wildcards": {},
            "endpoints": {
                "POST": {
                    "method": "POST",
                    "spec": "\/spec\/api-v2\/api\/2\/image\/base64\/POST\/.json",
                    "description": "Uploads a base64 encoded image resource.",
                    "parameters": {
                        "source": {
                            "name": "source",
                            "regex": "\/^[a-zA-Z0-9\\\/\\r\\n+]*={0,2}$\/",
                            "description": "A base64 encoded image string.",
                            "isRequired": true
                        }
                    }
                }
            }
        },
        "api-v2:\/api\/2\/image\/{id:\\d+}\/": {
            "name": "api-v2:\/api\/2\/image\/{id:\\d+}\/",
            "spec": "\/spec\/api-v2\/api\/2\/image\/{id:\\d+}\/route.json",
            "path": "\/api\/2\/image\/{id:\\d+}\/",
            "regex": "~^(?|\/api\/2\/image\/(\\d+)\/)$~",
            "wildcards": {
                "id": "^\\d+$"
            },
            "endpoints": {
                "GET": {
                    "method": "GET",
                    "spec": "\/spec\/api-v2\/api\/2\/image\/{id:\\d+}\/GET\/.json",
                    "description": "Get the image identified by its id.",
                    "parameters": {}
                }
            }
        },
        "api-v2:\/api\/2\/image\/binary\/": {
            "name": "api-v2:\/api\/2\/image\/binary\/",
            "spec": "\/spec\/api-v2\/api\/2\/image\/binary\/route.json",
            "path": "\/api\/2\/image\/binary\/",
            "regex": "#\/api\/2\/image\/binary\/#",
            "wildcards": {},
            "endpoints": {
                "POST": {
                    "method": "POST",
                    "spec": "\/spec\/api-v2\/api\/2\/image\/binary\/POST\/.json",
                    "description": "Uploads a binary image resource.",
                    "parameters": {
                        "source": {
                            "name": "source",
                            "regex": "\/^.*$\/",
                            "description": "A binary image.",
                            "isRequired": true,
                            "attributes": "tryFiles"
                        }
                    }
                }
            }
        },
        "api-v2:\/api\/2\/image\/url\/": {
            "name": "api-v2:\/api\/2\/image\/url\/",
            "spec": "\/spec\/api-v2\/api\/2\/image\/url\/route.json",
            "path": "\/api\/2\/image\/url\/",
            "regex": "#\/api\/2\/image\/url\/#",
            "wildcards": {},
            "endpoints": {
                "POST": {
                    "method": "POST",
                    "spec": "\/spec\/api-v2\/api\/2\/image\/url\/POST\/.json",
                    "description": "Uploads an image URL image resource.",
                    "parameters": {
                        "source": {
                            "name": "source",
                            "regex": "\/^(https?|ftp)+\\:\\\/\\\/.+$\/",
                            "description": "An image URL.",
                            "isRequired": true
                        }
                    }
                }
            }
        },
        "api-v2:\/api\/2\/album\/{id:\\d+}\/": {
            "name": "api-v2:\/api\/2\/album\/{id:\\d+}\/",
            "spec": "\/spec\/api-v2\/api\/2\/album\/{id:\\d+}\/route.json",
            "path": "\/api\/2\/album\/{id:\\d+}\/",
            "regex": "~^(?|\/api\/2\/album\/(\\d+)\/)$~",
            "wildcards": {
                "id": "^\\d+$"
            },
            "endpoints": {
                "GET": {
                    "method": "GET",
                    "spec": "\/spec\/api-v2\/api\/2\/album\/{id:\\d+}\/GET\/.json",
                    "description": "Get an album identified by its id.",
                    "parameters": {}
                },
                "PATCH": {
                    "method": "PATCH",
                    "spec": "\/spec\/api-v2\/api\/2\/album\/{id:\\d+}\/PATCH\/.json",
                    "description": "Updates an album identified by its id.",
                    "parameters": {}
                }
            }
        },
        "api-v2:\/api\/2\/album\/": {
            "name": "api-v2:\/api\/2\/album\/",
            "spec": "\/spec\/api-v2\/api\/2\/album\/route.json",
            "path": "\/api\/2\/album\/",
            "regex": "#\/api\/2\/album\/#",
            "wildcards": {},
            "endpoints": {
                "POST": {
                    "method": "POST",
                    "spec": "\/spec\/api-v2\/api\/2\/album\/POST\/.json",
                    "description": "Creates an album.",
                    "parameters": {
                        "name": {
                            "name": "name",
                            "regex": "\/\\w+\/",
                            "description": "",
                            "isRequired": true
                        }
                    }
                }
            }
        }
    }
}
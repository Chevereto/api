{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.php$",
                "cmd": "vendor/bin/ecs check ${file} --fix"
            }
        ]
    },
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        ".phpunit.cache": false,
        "build": false,
        "composer-dev.lock": false,
        ".phpunit.result.cache": false,
        "composer.lock": false,
        "ecs-chevere.php": false,
        ".github": false,
        ".vscode": false,
        ".gitattributes": false,
        ".gitignore": false,
        ".header": false,
        "composer-dev.json": false,
        "vendor": false,
        "phpunit-report.xml": false,
        "phpunit.xml": false,
        "LICENSE": false,
        "ecs.php": false,
        "DONORS.md": false,
        "LOGO.svg": false,
        "SPONSORS.md": false
    },
    "explorerExclude.backup": null
}